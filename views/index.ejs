<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Listing all <%= title %></p>

    <ul>
      <% messages.forEach(message => { %>
        <li>
          <h2><%= message.user %></h2>
          <p><%= message.text %></p>
          <b><%= message.creationDate %></b>
        </li>
      <%}) %>
    </ul>
  </body>
</html>
